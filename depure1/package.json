{
  "name": "depure-pro-django",
  "displayName": "Depure Pro (Django Edition)",
  "description": "The definitive AI-powered requirements manager for Django projects. Generate a complete and accurate requirements.txt with one click.",
  "version": "1.0.0",
  "publisher": "Anurag6569201",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Python"
  ],
  "activationEvents": [
    "onView:depureProView"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "depure.analyzeDjangoProject",
        "title": "Depure: Analyze Django Project",
        "icon": "$(rocket)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "depure.analyzeDjangoProject",
          "when": "view == depureProView",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "depure-pro-container",
          "title": "Depure Pro",
          "icon": "media/depure-icon.svg"
        }
      ]
    },
    "views": {
      "depure-pro-container": [
        {
          "id": "depureProView",
          "name": "Requirements Manager",
          "type": "webview"
        }
      ]
    },
    "configuration": {
      "title": "Depure Pro",
      "properties": {
        "depure.pro.llm.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your API key from Google AI Studio. Required for analysis."
        },
        "depure.pro.llm.model": {
          "type": "string",
          "default": "gemini-1.5-flash-latest",
          "description": "The Gemini model to use for analysis."
        },
        "depure.pro.excludePatterns": {
          "type": "array",
          "default": [ "**/node_modules/**", "**/.*/**", "**/venv/**", "**/.venv/**", "**/__pycache__/**", "**/*.egg-info/**", "**/migrations/**" ],
          "description": "Glob patterns for folders to exclude from analysis."
        }
      }
    }
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0"
  }
}